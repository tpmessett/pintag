<div class="navbar navbar-expand-sm fixed-top navbar-light navbar-lewagon">
      <div class="container">
        <%= link_to "/", class: "navbar-brand" do %>
          <span class="circle-icon-nav"><i class="fas fa-map-pin"></i></span>
        <% end %>

        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav mr-auto">
            <% if current_user %>
              <li class="nav-item dropdown">
                  <%= image_tag "https://res.cloudinary.com/dsp7ysucn/image/upload/v1614269335/hdswoqzf2hivzgu7todb.jpg", class: "avatar dropdown-toggle", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
                  <%= link_to "logout", destroy_user_session_path, method: :delete, class: "dropdown-item" %>
                </div>
              </li>
            <% else %>
              <li class="nav-item">
                <%= link_to "Sign Up", new_user_registration_path, class: "nav-link" %>
              </li>
              <li class="nav-item">
                <%= link_to new_user_registration_path, class: "navbar-brand" do %>
                  <span class="circle-icon-nav"><i class="fas fa-sign-in-alt"></i></span>
                <% end %>
              </li>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
